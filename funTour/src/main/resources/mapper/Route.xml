<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.funtour.mapper.RouteMapper">
    <select id="findRoutesByProvinceId" resultType="com.funtour.entity.Route">
        SELECT *
        FROM route
        WHERE province_id = #{provinceId}
    </select>
</mapper>